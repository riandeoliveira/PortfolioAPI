<!DOCTYPE html>

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta name="author" content="Rian Oliveira" />
  <meta name="description" content="AspNetTemplate API" />
  <meta name="keywords" content="" />
  <meta name="theme-color" content="#ffffff" />
  <title></title>
  <style>
    body {
      font-family: Arial, sans-serif;
      color: #2d3748;
      background-color: #dae5ec;
    }

    .flex-container {
      margin: 24px auto;
      max-width: 800px;
    }

    .flex-item {
      background-color: #ffffff;
      border-radius: 10px;
      padding: 24px;
    }

    h1 {
      font-size: 30px;
      font-weight: 700;
      text-align: center;
    }

    a {
      color: #3b82f6;
    }

    .button-container {
      margin: 24px 0;
    }

    .button {
      background-color: #3b82f6;
      color: #ffffff;
      padding: 12px 24px;
      border-radius: 10px;
    }

    .button:hover {
      background-color: #4299e1;
    }
  </style>
</head>

@RenderBody()

</html>
